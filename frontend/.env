# تكوين بيئة التشغيل لنظام Adalalegalis - الواجهة الأمامية
# ملاحظة: يجب تغيير هذه القيم في بيئة الإنتاج

# معلومات التطبيق
NODE_ENV=development
PORT=4200

# عنوان الواجهة الخلفية
API_URL=http://localhost:3000

# تكوين الأمان
ENABLE_CSP=true
ENABLE_HTTPS=false

# تكوين التخزين المؤقت
ENABLE_SERVICE_WORKER=true
CACHE_API_RESPONSES=true

# تكوين الأداء
ENABLE_BUNDLE_ANALYZER=false
ENABLE_COMPRESSION=true
ENABLE_LAZY_LOADING=true
ENABLE_TREE_SHAKING=true

# تكوين التحليلات
ENABLE_ANALYTICS=false
GOOGLE_ANALYTICS_ID=

# تكوين إمكانية الوصول
ENABLE_ACCESSIBILITY_FEATURES=true
DEFAULT_FONT_SIZE=medium
DEFAULT_CONTRAST=normal
DEFAULT_ANIMATIONS=enabled

# تكوين اللغة
DEFAULT_LANGUAGE=ar
RTL_ENABLED=true

# تكوين الموضوع
DEFAULT_THEME=light
ENABLE_DARK_MODE=true

# تكوين التكاملات
GOOGLE_MAPS_API_KEY=
STRIPE_PUBLIC_KEY=pk_test_your_stripe_public_key

# تكوين التسجيل
LOG_LEVEL=info
ENABLE_ERROR_REPORTING=true

# تكوين المصادقة
AUTH_STRATEGY=jwt
ENABLE_SOCIAL_LOGIN=false
ENABLE_2FA=true

# تكوين الأداء المتقدم
ENABLE_SSR=false
ENABLE_PWA=true
ENABLE_PRELOADING=true
ENABLE_PREFETCHING=true
